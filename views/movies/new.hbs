<h2>New Movie!</h2>
<form action="/movies" method="POST">
  <p>Title:<input type="text" name='title'></p>
  <p>Genre:<input type="text" name='genre'></p>
  <p>Plot:<input type="text" name='plot'></p>
  <select name="cast" id="" multiple>
    {{#each Celebslist}}
    <option value="{{this._id}}">{{this.name}}</option>
    {{/each}}

  </select>

  
  <p><button>Submit that stuff!</button></p>
</form>